@{
  Layout = "_Layout";
}
<div class="center">
  <div class="move">
    <div class="test">
      <p>List of Specialties</p>
      <table>
        <tr>
          <th>Specialty No.</th>
          <th>NAME</th>
        </tr>
        @foreach(var specialty in Model)
        {
        <tr>
          <td>@specialty.GetSpecialtyId()</td>
          <td>@specialty.GetSpecialtyName()</td>
        </tr>
        }
      </table>
      <p><a href='/salon/new'>Add new stylist<a/></p>
      <p><a href='/'>Home<a/></p>
    </div>
  </div>
</div>
